{
  "kind": "collectionType",
  "collectionName": "team_members",
  "info": {
    "singularName": "team-member",
    "pluralName": "team-members",
    "displayName": "Team Member"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "position": {
      "type": "string",
      "required": true
    },
    "avatar": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "slug": {
      "type": "uid",
      "required": true
    },
    "featured": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "featured_order": {
      "type": "integer",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "featured"
            },
            true
          ]
        }
      },
      "required": true,
      "min": 0
    },
    "suffix": {
      "type": "string"
    },
    "description": {
      "type": "richtext",
      "required": false
    }
  }
}
